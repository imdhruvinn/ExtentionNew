{
  "manifest_version": 3,
  "name": "Mental Health Analyzer",
  "version": "1.0",
  "description": "Analyzes web page content for emotional indicators.",
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
